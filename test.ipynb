    "import cmath\n",
    "import numpy as np\n",
    "\n",
    "M = 8\n",
    "N = 8"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "dft2d = np.zeros((M,N),dtype=complex)\n",
    "for i in range(M):\n",
    "    for j in range(N):\n",
    "        dft2d[i,j] = cmath.exp(- 2j * np.pi *i*j/N)\n",
    "                "
   ]
